{
  "$schema": "./node_modules/turbo/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint:check": {
      "dependsOn": [
        "^lint:check"
      ]
    },
    "lint:format": {
      "dependsOn": [
        "^lint:format"
      ]
    },
    "test:e2e": {
      "dependsOn": [
        "^test:e2e"
      ],
      "env": [
        "CI",
        "PORT"
      ]
    },
    "test:unit": {
      "dependsOn": [
        "^test:unit"
      ]
    },
    "types:check": {
      "dependsOn": [
        "^types:check"
      ]
    },
    "types:generate": {
      "dependsOn": [
        "^types:generate"
      ]
    }
  },
  "ui": "tui"
}
